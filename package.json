{
  "name": "ao-tokens",
  "version": "0.0.4",
  "repository": "https://github.com/martonlederer/ao-tokens",
  "author": "Marton Lederer <marton@lederer.hu>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "development": "./dist/index.development.js",
        "default": "./dist/index.js"
      },
      "require": {
        "development": "./dist/index.development.cjs",
        "default": "./dist/index.cjs"
      }
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "jest"
  },
  "devDependencies": {
    "@permaweb/aoconnect": "^0.0.62",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "arweave": "^1.15.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@permaweb/aoconnect": ">= 0.0.51 < 1"
  }
}
